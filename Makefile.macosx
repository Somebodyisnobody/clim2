include Makefile.defs

SHARED_EXT = dylib

SHARED-XM-OBJS= climxm.dylib
SHARED-OL-OBJS= climol.dylib

Makefile=Makefile.macosx

# removed /usr/local/lib:
SET_LIBRARY_PATH = \
  DYLD_LIBRARY_PATH=/usr/X11R6/lib:/lib:/usr/lib:$(DYLD_LIBRARY_PATH); \
  export DYLD_LIBRARY_PATH

#CL=../src/xm-dcl

TKLIB=-L/usr/local/motif/lib -lXm
XTLIB=-L/usr/X11R6/lib -lXt
XLIB=-L/usr/X11R6/lib -lX11
# removed /usr/local/lib/liblanginfo.a from XLIB

XINCLUDES=-I/usr/X11R6/include -I/usr/local/motif/include

CFLAGS= 
PICFLAGS = 
SHAREFLAGS = 
MAKE_SHARED = sh ../src/bin/make_shared.mac
AR = ar cq

PRODUCT-OBJS= $(PRODUCT-GENERIC-OBJS) $(STATIC-XM-OBJS) $(SHARED-XM-OBJS)

IMPORTS = stub-x.o stub-xt.o stub-motif.o

include Makefile.generic
